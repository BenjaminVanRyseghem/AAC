package tsp;

public class Arrete implements Comparable<Arrete> {

	private double value;
	private int src;
	private int dest;
		
	@Override
	public int compareTo(Arrete arg) {
		return (int)(value-arg.getValue());
	}

	public Arrete(double value, int src, int dest) {
		this.value = value;
		this.src = src;
		this.dest = dest;
	}

	public double getValue() {
		return value;
	}

	public int getSrc() {
		return src;
	}

	public int getDest() {
		return dest;
	}
	
}
