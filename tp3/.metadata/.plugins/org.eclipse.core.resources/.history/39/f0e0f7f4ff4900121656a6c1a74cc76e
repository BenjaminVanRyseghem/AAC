package tsp;

public class Arrete implements Comparable<Double> {

	private double value;
	private int src;
	private int dest;
		
	@Override
	public int compareTo(Double arg) {
		return (int)(arg-this.value);
	}

	public Arrete(double value, int src, int dest) {
		this.value = value;
		this.src = src;
		this.dest = dest;
	}
}
